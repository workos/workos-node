{
  "version": "8.0.0-beta.2",
  "name": "@workos-inc/node",
  "author": "WorkOS",
  "description": "A Node wrapper for the WorkOS API",
  "homepage": "https://github.com/workos-inc/workos-node",
  "license": "MIT",
  "keywords": [
    "workos"
  ],
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workos-inc/workos-node.git"
  },
  "bugs": {
    "url": "https://github.com/workos-inc/workos-node/issues"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:worker": "jest src/worker.spec.ts",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\" --check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "iron-session": "^8.0.4",
    "jose": "~5.6.3",
    "leb": "^1.0.0",
    "pluralize": "8.0.0",
    "qs": "6.14.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.21.0",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.14",
    "@types/node": "~18",
    "@types/pluralize": "0.0.33",
    "@typescript-eslint/parser": "^8.25.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^17.15.1",
    "jest": "29.7.0",
    "jest-environment-miniflare": "^2.14.2",
    "jest-fetch-mock": "^3.0.3",
    "nock": "^13.5.5",
    "prettier": "^3.5.3",
    "supertest": "7.1.0",
    "ts-jest": "29.3.1",
    "tsup": "^8.5.0",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.25.0"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "workerd": {
        "import": "./lib/index.worker.js",
        "require": "./lib/index.worker.cjs"
      },
      "edge-light": {
        "import": "./lib/index.worker.js",
        "require": "./lib/index.worker.cjs"
      },
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    },
    "./worker": {
      "types": "./lib/index.worker.d.ts",
      "import": "./lib/index.worker.js",
      "require": "./lib/index.worker.cjs",
      "default": "./lib/index.worker.js"
    },
    "./package.json": "./package.json"
  }
}
