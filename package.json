{
  "version": "0.1.0-canary.0",
  "name": "@workos-inc/js",
  "author": "WorkOS",
  "description": "A wrapper for the WorkOS API for all things javascript.",
  "homepage": "https://github.com/workos-inc/workos-js#readme",
  "license": "MIT",
  "keywords": [
    "workos"
  ],
  "main": "dist/src/workos.js",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workos-inc/workos-js.git"
  },
  "bugs": {
    "url": "https://github.com/workos-inc/workos-js/issues"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\" --check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "prepare": "yarn run build",
    "bump-version": " auto-upgrade-npm-version",
    "preversion": "yarn run lint"
  },
  "dependencies": {
    "axios": "0.19.0",
    "pluralize": "8.0.0",
    "query-string": "6.10.1"
  },
  "devDependencies": {
    "@types/jest": "25.1.1",
    "@types/node": "12.12.28",
    "@types/pluralize": "0.0.29",
    "auto-upgrade-npm-version": "1.0.7",
    "axios-mock-adapter": "1.17.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "tslint": "5.20.1",
    "typescript": "3.8.2"
  }
}
