{
  "name": "@workos-inc/js",
  "version": "0.0.3",
  "main": "dist/src/workos.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\" --check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "prepare": "yarn run build",
    "bump-version": " auto-upgrade-npm-version",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint"
  },
  "dependencies": {
    "axios": "0.19.0",
    "pluralize": "8.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.16",
    "@types/node": "12.6.1",
    "@types/pluralize": "0.0.29",
    "auto-upgrade-npm-version": "1.0.7",
    "axios-mock-adapter": "1.17.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "description": "A wrapper for the WorkOS API for all things javascript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workos-inc/workos-js.git"
  },
  "keywords": [
    "workos"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/workos-inc/workos-js/issues"
  },
  "homepage": "https://github.com/workos-inc/workos-js#readme"
}
